{
  "name": "vragovr/notification-bundle",
  "description": "Simple email notifications bundle for Symfony.",
  "version": "1.0.0",
  "type": "library",
  "keywords": [
    "symfony",
    "bundle",
    "swiftmailer",
    "email"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "symfony/framework-bundle": "^3.0|^4.0",
    "symfony/twig-bundle": "^3.0|^4.0",
    "symfony/swiftmailer-bundle": "^3.0",
    "tijsverkoyen/css-to-inline-styles": "^2.0",
    "html2text/html2text": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "NotificationBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NotificationBundle\\": "tests/"
    }
  }
}
